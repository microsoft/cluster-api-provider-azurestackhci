{
  "build": {
    "content": [
      {
        "files": ["**/*.md", "**/toc.yml", "**/*.vsdx"],
        "exclude": "_site/**"
      }
    ],
    "resource": [
      {
        "files": ["**/media/**", "**/images/**", "**/*.{jpg,png,svg}"]
      }
    ],
    "output": "_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": ["default", "modern"],
    "postProcessors": [],
    "keepFileLink": false,
    "disableGitFeatures": false
  }
}
